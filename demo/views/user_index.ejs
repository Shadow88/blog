<%include layout.ejs%>
<ol class="breadcrumb">
    <li><a href="/admin/user">用户首页</a></li>
    <li><span>用户列表</span></li>
</ol>
<h3>用户列表</h3>
<table class="table table-striped table-hover">
    <tr class="warning">
        <th>ID</th>
        <th>用户名</th>
        <th>密码</th>
        <th>是否为管理</th>
    </tr>

        <% user.map(function (item) {%>
    <tr>
        <td><%= item._id %></td>
        <td><%= item.username %></td>
        <td><%= item.password %></td>
        <td><%if(item.isAdmin) {%>是<%} else{%>否<%}%></td>
    </tr>
       <% })%>


</table>
<% include page.ejs%>
